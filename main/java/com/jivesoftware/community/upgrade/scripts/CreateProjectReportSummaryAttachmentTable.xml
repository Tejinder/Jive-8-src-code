<?xml version="1.0"?>

<!-- Table definition for Synchro New Requirements -->
<schema name="CreateProjectReportSummaryAttachmentTable">
    <table name="grailProjectRepSummaryAttach" description="New Project Report Summary Details Table">
        <column name="projectID" type="bigint" nullable="false" description="Project ID"/>
        <column name="reportType" type="int" nullable="true" description="Report Type"/>
        <column name="reportOrderId" type="bigint" nullable="true" description="Report Order Id"/>
        <column name="attachmentId" type="bigint" nullable="true" description="Attachment Id"/>
        
        <index type="normal" name="grailProjectRepSummAttach_Idx" column="projectID"/>
    </table>

</schema>